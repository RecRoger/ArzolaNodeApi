{
  "name": "coderhouse-roger-node",
  "version": "1.0.1",
  "description": "node server project from coderhouse class",
  "main": "server.js",
  "type": "module",
  "engines": {
    "node": "14.17.6",
    "npm": "6.14.15"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "cluster": "nodemon server.js 8081 CLUSTER",
    "forever:start": "forever start server.js",
    "forever:stop": "forever stopall",
    "pm2:start": "pm2 start server.js --name=\"RecRoger_Project\" --watch",
    "pm2:cluster": "pm2 start server.js --name=\"RecRoger_Project\" -i max -- 8081",
    "pm2:restart": "pm2 restart \"RecRoger_Project\"",
    "pm2:stop": "pm2 stop \"RecRoger_Project\"",
    "pm2:logs": "pm2 logs",
    "artilleryTest": "artillery quick --count 10 -n 20 'http://localhost:8080/info' > FORK.txt",
    "autocannon": "node autocannon.js",
    "0x": "0x server.js 8081",
    "axios": "node test/clientProducts.js",
    "test": "mocha test/test.js"
  },
  "author": "RecRoger",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^7.3.0",
    "autocannon": "^7.9.0",
    "axios": "^1.0.0",
    "compression": "^1.7.4",
    "connect-mongo": "^4.6.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-handlebars": "^6.0.6",
    "express-session": "^1.17.3",
    "forever": "^4.0.3",
    "handlebars": "^4.7.7",
    "log4js": "^6.6.1",
    "mongoose": "^6.4.4",
    "multer": "^1.4.4",
    "mysql": "^2.18.1",
    "nodemailer": "^6.7.8",
    "nodemon": "^2.0.16",
    "passport": "^0.5.3",
    "passport-local": "^1.0.0",
    "pm2": "^5.2.0",
    "socket.io": "^4.5.1",
    "twilio": "^3.82.0"
  },
  "devDependencies": {
    "mocha": "^10.0.0"
  }
}